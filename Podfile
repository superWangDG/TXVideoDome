#use_framework!
#use_modular_headers
# swift 语言需要添加以上
# inhibit_all_warnings! 不显示 三方库的警告
platform:ios, “8.0”
inhibit_all_warnings!
target 'TXVideoDome' do

    #支持框架

#pod 'GSPlayer' #视频播放
#pod 'SJBaseVideoPlayer', :git => 'https://gitee.com/changsanjiang/SJBaseVideoPlayer.git' #视频播放 OC
#pod 'SJVideoPlayer'
pod 'SJBaseVideoPlayer'

pod 'SwiftyJSON' #解析Json 数据

pod 'DeviceKit' # 获取设备电量等
pod 'Alamofire' #网络请求框架
pod 'AFNetworking' #网络请求框架(上传图片)

pod 'Kingfisher' #网络加载图片

pod 'YYText'   #文本
pod 'YYAsyncLayer'
pod 'YYCategories'

pod 'NVActivityIndicatorView' #加载动画

# Objective-C 语言
pod 'ZLPhotoBrowser'  #相册选择

pod 'CLPlayer'  #视频播放器

# 网页加载

#腾讯短视频

pod 'BlocksKit' #之后自定义时删除
pod 'JPVideoPlayer' # 播放器

pod 'Masonry'
pod 'TYPagerController'

pod 'WMPlayer'

pod 'MJRefresh' #下拉上拉 加载数据
end


